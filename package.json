{
  "name": "@flybondi/serverless-rollup-plugin",
  "version": "0.0.0-development",
  "description": "A rollup plugin for serverless",
  "main": "index.js",
  "author": "Flybondi <dev@flybondi.com>",
  "license": "UNLICENSED",
  "private": false,
  "keywords": [
    "serverless",
    "1.0",
    "rollup",
    "es6",
    "plugin",
    "yarn",
    "flybondi"
  ],
  "scripts": {
    "lint": "eslint index.js src/**",
    "prettier": "prettier --write index.js",
    "start": "serverless offline start --environment development",
    "test.coverage": "yarn test --coverage --coverageDirectory .coverage",
    "test.watch": "jest --watchAll",
    "test": "jest"
  },
  "peerDependencies": {
    "rollup": "^2.5.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "commitlint": "^8.3.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^4.2.5",
    "jest": "^25.5.2",
    "lint-staged": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rollup": "^2.7.5",
    "serverless": "^1.35.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "run-s test"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "fs-extra": "^9.0.0",
    "ramda": "^0.27.0",
    "yup": "^0.28.4"
  }
}
